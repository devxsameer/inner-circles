<a href="/posts/create">Create New Post</a>
<h1 class="text-2xl">My Posts</h1>

<ol class="flex flex-col gap-4 py-4">
    {{#each posts}}
    <li class="shadow-md p-2">
        <ul>
            <li>Title: {{this.title}} </li>
            <li>Body: {{this.body}}</li>
            <li>Circle Name: {{this.circleName }}</li>
        </ul>
        <a href="/posts/{{this.id}}/delete">Delete Post</a>
    </li>
    {{/each}}
</ol>